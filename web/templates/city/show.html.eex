<h2>Show city</h2>

<ul>

    <li>
      <strong>Name:</strong>
      <%= @city.name %>
    </li>

    <li>
      <strong>Landlords:</strong>
      <%= @num_landlords %>
    </li>

    <li>
      <strong>Houses:</strong>
      <%= @num_houses %>
    </li>

</ul>

<%= link "Edit", to: city_path(@conn, :edit, @city) %>
<%= link "Back", to: city_path(@conn, :index) %>
